{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@autocode-ts/ac-angular": ["dist/packages/angular/ac-angular"],
      "@autocode-ts/ac-data-dictionary": [
        "packages/common/ac-data-dictionary/index.ts"
      ],
      "@autocode-ts/ac-datagrid": ["packages/browser/ac-datagrid/src/index.ts"],
      "@autocode-ts/ac-extensions": ["packages/common/ac-extensions/index.ts"],
      "@autocode-ts/ac-ng-data-dictionary-editor": [
        "packages/angular/ac-ng-data-dictionary-editor/index.ts"
      ],
      "@autocode-ts/ac-ng-datagrid": [
        "packages/angular/ac-ng-datagrid/src/index.ts"
      ],
      "@autocode-ts/ac-ng-datagrid-on-aggrid": [
        "packages/angular/ac-ng-datagrid-on-aggrid/index.ts"
      ],
      "@autocode-ts/ac-ng-drag-drop-builder": [
        "packages/angular/ac-ng-drag-drop-builder/index.ts"
      ],
      "@autocode-ts/ac-ng-input-audio-recording": [
        "packages/angular/ac-ng-input-audio-recording/index.ts"
      ],
      "@autocode-ts/ac-ng-input-on-ngselect": [
        "packages/angular/ac-ng-input-on-ngselect/index.ts"
      ],
      "@autocode-ts/ac-ng-input-on-quill": [
        "packages/angular/ac-ng-input-on-quill/index.ts"
      ],
      "@autocode-ts/ac-ng-input-on-tinymce": [
        "packages/angular/ac-ng-input-on-tinymce/index.ts"
      ],
      "@autocode-ts/ac-ng-media-library": [
        "packages/angular/ac-ng-media-library/src/index.ts"
      ],
      "@autocode-ts/ac-ng-menu-editor": [
        "packages/angular/ac-ng-menu-editor/src/index.ts"
      ],
      "@autocode-ts/ac-ng-node-editor": [
        "packages/angular/ac-ng-node-editor/index.ts"
      ],
      "@autocode-ts/ac-ng-runtime": [
        "packages/angular/ac-ng-runtime/src/index.ts"
      ],
      "@autocode-ts/ac-react": ["packages/react/ac-react/index.ts"],
      "@autocode-ts/ac-sql": ["packages/node/ac-sql/index.ts"],
      "@autocode-ts/ac-vue": ["packages/vue/ac-vue/index.ts"],
      "@autocode-ts/ac-web": ["packages/node/ac-web/index.ts"],
      "@autocode-ts/ac-web-on-express": [
        "packages/node/ac-web-on-express/index.ts"
      ],
      "@autocode-ts/autocode": ["packages/common/autocode/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
