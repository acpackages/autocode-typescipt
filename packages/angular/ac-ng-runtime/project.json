{
  "name": "ac-ng-runtime",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/angular/ac-ng-runtime/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:ng-packagr",
      "outputs": ["{options.outputPath}"],
      "options": {
        "project": "packages/angular/ac-ng-runtime/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/angular/ac-ng-runtime/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/angular/ac-ng-runtime/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/angular/ac-ng-runtime/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
